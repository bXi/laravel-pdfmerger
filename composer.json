{
    "name": "bxi/laravel-pdfmerger",
    "type": "library",
    "description": "Generic PDF merger for Laravel",
    "keywords": [
        "bXi",
        "laravel",
        "pdf",
        "merger",
        "pdfmerger"
    ],
    "homepage": "https://github.com/bXi/pdfmerger",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Meinen",
            "email": "info@bixxy.net",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.0",
        "setasign/fpdi": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "bXi\\PDFMerger\\": "src/PDFMerger"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bXi\\PDFMerger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
